{% extends "base.twig" %}

{% block content %}
	<article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
		<section class="article-content">
			<h1 class="my-5">{{post.title}}</h1>
			<div class="content article-body">
				{{post.content}}
			</div>
		</section>
	</article>
	<!-- /content-wrapper -->
{% endblock %}
